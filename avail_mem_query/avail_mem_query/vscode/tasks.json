{
    "tasks": [
        // Link all the object files into the executable.
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-11 生成活动文件",
            "command": "/usr/bin/gcc-11",
            "args": [
                "${workspaceFolder}/build/main.o",
                "${workspaceFolder}/build/hello.o",
                "-o",
                "avail_mem_query",
                "-fdiagnostics-color=always",
                "-ldl",
                "-lm",
                "-lstdc++"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}
